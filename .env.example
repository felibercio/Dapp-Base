# Configurações da aplicação
REACT_APP_NAME=Super Dapp Base
REACT_APP_VERSION=1.0.0

# Configurações da rede Base
REACT_APP_BASE_CHAIN_ID=8453
REACT_APP_BASE_RPC_URL=https://mainnet.base.org
REACT_APP_BASE_EXPLORER_URL=https://basescan.org

# Configurações do PIX API
REACT_APP_PIX_API_URL=https://api.bacen.gov.br/pix/v1
REACT_APP_PIX_CLIENT_ID=your_pix_client_id
REACT_APP_PIX_CLIENT_SECRET=your_pix_client_secret

# Configurações do WalletConnect
REACT_APP_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Configurações do contrato BRLA
REACT_APP_BRLA_CONTRACT_ADDRESS=0x...
REACT_APP_BRLA_DECIMALS=18

# Configurações de desenvolvimento
REACT_APP_DEBUG=true
REACT_APP_ENVIRONMENT=development
